<header>
	<div class="navbar">
		<div class="navbar-inner">
			<ul class="nav">
				<% menu.forEach(function(item){ %>
					<li class="<%= route===item.url ? 'active' : '' %>"><a href="<%= item.url %>"><%= item.title %></a></li>
				<% }) %>
			</ul>
		</div>
	</div>
	<%=route %>
	<%-blocks.header %>
</header>